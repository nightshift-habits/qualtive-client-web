{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "allowJs": true,
    "isolatedModules": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "declaration": true,
    "jsx": "react-jsx",
    "jsxImportSource": "qualtive-client-web-jsx"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
